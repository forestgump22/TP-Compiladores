//EJEMPLO 1
estructura Punto {
    entero x;
    entero y;
    flotante z;
    cadena palabra;
};

Punto punto1, punto2;
punto1.x = 5;
punto1.y = 10;
punto1.z = 4.55;
punto2.x = 15;
punto2.y = 20;
punto2.palabra = coordenada;

//EJEMPLO 2
estructura Rectangulo {
    flotante largo;
    flotante ancho;
};

Rectangulo rect1;
rect1.largo = 12.5;
rect1.ancho = 7.3;

flotante area;
area = rect1.largo * rect1.ancho;

//EJEMPLO 3
entero num1, num2, num3;
num1 = 10;
num2 = 20;
num3 = 30;

